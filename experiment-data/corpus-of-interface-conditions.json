{"num_experiments_to_run": 1,
 "check_model": false,
 "smt.phase_selection.random_seed.enabled": true,
 "max_num_empty_lexical_items": 2,
 "max_num_movements": 2,
 "max_num_head_movements": 2,
 "max_num_features_per_lexical_entry": 3,
 "num_overt_lexical_entries_per_form": 2,
 "num_covert_lexical_entries_per_form": 5,
 "max_num_overt_pf_connections": 1,
 "max_num_covert_pf_connections": 6,
 "selection_feature_labels": ["w", "x", "y", "z"],
 "licensing_feature_labels": ["p", "q", "r"],
 "wisdom": {},
 "init_from_lexicon": false,
 "init_lexicons": [],
 "initial_lexical_items": "[]",
 "ics_to_use": "0-7",
 "evaluate_intermediate_steps": true,
 "input_sequence": [

     {"sentence": "what has the man eaten ?",
      "locality_constraints": [["theta", {"pred": "eaten", "subj": "the man", "obj": "what"}],
			       ["agree", {"pred": "has", "subj": "the man"}]],
      "categorical_constraints": {"N": ["man"],
				  "V": ["eaten"]}},

     {"sentence": "was she given money ?",
      "locality_constraints": [["theta", {"pred": "given", "iobj": "she", "obj": "money"}],
			       ["agree", {"pred": "was", "subj": "she"}]],
      "categorical_constraints": {"N": ["she", "money"],
				  "V": ["given"]}},

     {"sentence": "who will tell her that he has resigned ?",
      "locality_constraints": [["theta", {"pred": "tell", "subj": "who", "obj": "that he has resigned", "iobj": "her"}],
			       ["agree", {"pred": "will", "subj": "who"}],
			       ["theta", {"pred": "resigned", "subj": "he"}],
			       ["agree", {"pred": "has", "subj": "he"}]],
      "categorical_constraints": {"N": ["her", "he"],
				  "V": ["tell", "resigned"]}},
     
     {"sentence": "she has known everyone who was loved .",
      "locality_constraints": [["theta", {"pred": "known", "subj": "she", "obj": "everyone who was loved"}],
			       ["agree", {"pred": "has", "subj": "she"}],
			       ["theta", {"pred": "loved", "obj": "everyone"}],
			       ["agree", {"pred": "was", "subj": "everyone"}]],
      "categorical_constraints": {"N": ["she", "everyone"],
				  "V": ["known", "loved"]}},
     
     {"sentence": "she knows that john has given money .",
      "locality_constraints": [["theta", {"pred": "knows", "subj": "she", "obj": "that john has given money"}],
			       ["theta", {"pred": "given", "subj": "john", "obj": "money"}],
			       ["agree", {"pred": "has", "subj": "john"}]],
      "categorical_constraints": {"N": ["she", "john", "money"],
				  "V": ["knows", "given"]}},

     {"sentence": "john has given money that was stolen .",
      "locality_constraints": [["theta", {"pred": "given", "subj": "john", "obj": "money that was stolen"}],
			       ["agree", {"pred": "has", "subj": "john"}],
			       ["theta", {"pred": "stolen", "obj": "money"}],
			       ["agree", {"pred": "was", "subj": "money"}]],
      "categorical_constraints": {"N": ["john", "money"],
				  "V": ["given", "stolen"]}},

     {"sentence": "john fears everyone who knows her .",
      "locality_constraints": [["theta", {"pred": "fears", "subj": "john", "obj": "everyone who knows her"}],
			       ["theta", {"pred": "knows", "subj": "everyone", "obj": "her"}]],
      "categorical_constraints": {"N": ["john", "everyone", "her"],
				  "V": ["fears", "knows"]}},

     {"sentence": "john fears that money was stolen .",
      "locality_constraints": [["theta", {"pred": "fears", "subj": "john", "obj": "that money was stolen"}],
			       ["theta", {"pred": "stolen", "obj": "money"}],
			       ["agree", {"pred": "was", "subj": "money"}]],
      "categorical_constraints": {"N": ["john", "money"],
				  "V": ["fears", "stolen"]}}
 ],
 "scrap material": []
}
